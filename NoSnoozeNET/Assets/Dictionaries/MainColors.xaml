<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="LabelBrush" Color="Black"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="Black"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="White"/>
    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="Transparent"/>
    <!--Shadow Properties-->
    <SolidColorBrush x:Key="ShadowColor" Color="Black"/>
    <DropShadowEffect x:Key="ControlShadow" Color="Black" BlurRadius="10" Direction="0" ShadowDepth="1"/>
</ResourceDictionary>